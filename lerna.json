{
  "version": "0.0.75",
  "npmClient": "npm",
  "command": {
    "publish": {
      "ignoreChanges": [
        "ignored-file",
        "*.md"
      ],
      "message": "chore(release): publish"
    },
    "bootstrap": {
      "ignore": "component-*",
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  },
  "packages": [
    "algorithm/*",
    "api/*",
    "compiler",
    "convert/*",
    "data/*",
    "doc",
    "example/*",
    "geometry/*",
    "math/*",
    "sys"
  ]
}
