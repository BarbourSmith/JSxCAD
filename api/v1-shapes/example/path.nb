md`# Path Construction`;

const arc = Arc(20, 300).gridView().md('A radius 20 Arc of 45 degrees.');

const hershey = Hershey(10)('Hershey')
  .page({ itemMargin: 5 })
  .gridView()
  .md('Single line text of size 10.');

const line = Line(10).topView().md('A line of length 10.');

const path = Path(Point(0), Point(10, 5), Point(20))
  .gridView()
  .md('A simple open path.');

const spiral = Spiral((a) => [[a / 1000]], { to: 360 * 10 })
  .gridView()
  .md('A spiral path.');

const wave = Wave((a) => [[0, sin(a * 3 * 10) * 10]], {
  from: -18,
  to: 18,
  by: 0.1,
})
  .gridView()
  .md('A sine wave path.');
