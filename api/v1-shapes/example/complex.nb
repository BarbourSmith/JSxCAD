md`# Complex Construction`;

md`## Assembly(...shapes)`;
const assembly = Assembly(Box(d(20)), Triangle(r(15)))
  .topView(50)
  .md(`An assembly of a 20x20 square and a triangle of length 5.`);

const assemblyPacked = Assembly(Box(d(20)), Triangle(r(15)))
  .pack()
  .topView(50)
  .md('The same assembly packed');

md`## Difference(...shapes)`;
const difference = Difference(Box(d(20)), Triangle(d(15)))
  .fill()
  .topView(50)
  .md('A 20x20 square with a triangle of size 5 removed.');

md`## Group(...shapes)`;
const group = Group(
  Box(d(20)).fill().color('red'),
  Triangle(d(20)).fill().color('blue')
).topView(50);

md`## Intersection(...shapes)`;
const intersection = Intersection(Box(d(10)).pull(10 / 2, 10 / -2), Ball(7))
  .view(50)
  .md('The intersection of a 10x10x10 Cube and a Sphere of radius 7.');

md`## Union(...shapes)`;
const union = Union(Triangle(5), Circle(4)).topView(50);
md`A union of a triangle and a circle.`;
